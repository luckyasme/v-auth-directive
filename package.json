{
  "name": "v-auth-directive",
  "version": "1.0.0",
  "description": "a vue permission directive",
  "exports": {
    ".": {
      "types": "./dist/type/index.d.ts",
      "require": "./dist/index.umd.js",
      "import": "./dist/index.mjs"
    }
  },
  "keywords": [
    "v-auth-directive",
    "vue",
    "vue3",
    "v-auth",
    "v-permission",
    "v-has"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "type": "module",
  "types": "./dist/type/index.d.ts",
  "files": [
    "dist",
    "README-zh-cn.md",
    "tsconfig.json"
  ],
  "scripts": {
    "dev": "rollup -w -c",
    "build": "node scripts/prebuild && rollup -c",
    "publish": "node scripts/prebuild && rollup -c && npm publish"
  },
  "author": "luckyasme",
  "repository": {
    "type": "git",
    "url": "https://github.com/luckyasme/v-auth-directive.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.11.18",
    "rollup": "^3.12.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vue": "^3.2.45",
    "vue-demi": "^0.13.11"
  }
}
